#!/home/quattro/Documentos/python/Wallpaper-Automater/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'isort','console_scripts','isort'
__requires__ = 'isort'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('isort', 'console_scripts', 'isort')()
    )
